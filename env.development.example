# Configuraci√≥n de Desarrollo - PedidoList
# Copia este archivo como .env para configurar las variables de entorno

# PWA Configuration
VITE_PWA_DISABLED=false

# Backend Configuration
VITE_BACKEND_URL=http://localhost:3030

# Environment
VITE_ENVIRONMENT=development
NODE_ENV=development

# Supabase Configuration (usar las mismas que en el backend)
VITE_SUPABASE_URL=https://your-supabase-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Stripe Configuration (usar clave de prueba)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Feature Flags
VITE_ENABLE_DEBUG=true
VITE_OAUTH_PERF_LOGS=true
